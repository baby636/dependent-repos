{
  "name": "repos-using-electron",
  "description": "A LevelDB with metadata from every GitHub repository that depends on Electron",
  "version": "2.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "bottleneck": "^2.1.0",
    "cool-story-repo": "^1.2.0",
    "csv-parser": "^2.0.0",
    "dotenv-safe": "^5.0.1",
    "ellipsize": "^0.1.0",
    "is-url": "^1.2.4",
    "jest": "^28.0.0",
    "make-promises-safe": "^1.1.0",
    "repo-images": "^1.0.1",
    "request-image-size": "^2.1.0",
    "standard": "^11.0.0"
  },
  "dependencies": {
    "husky": "^3.0.5",
    "level": "^5.0.1",
    "yaml-lint": "^1.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "scripts": {
    "lint": "standard && yamllint ./.github/workflows/schedule.yml",
    "fetch-repo-names": "./script/fetch-repo-names",
    "build": "npm run collect && npm run icons",
    "collect": "node script/collect.js",
    "icons": "node script/icons.js",
    "test": "jest"
  }
}
